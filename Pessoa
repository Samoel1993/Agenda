public abstract class Pessoa {
    private String cpf;
    private String nome;
    private int tipo;
    private Contato contato;

    public Pessoa(String cpf, String nome, int tipo, Contato contato){
        this.cpf = cpf;
        this.nome = nome;
        this.tipo = tipo;
        this.contato = contato;
    }

    public void setCpf(String cpf){
        this.cpf = cpf;
    }

    public String getCpf(){
        return cpf;
    }

    public void setNome(String nome){
        this.nome = nome;
    }

    public String getNome(){
        return nome;
    }

    public void setTipo(int tipo){
        this.tipo = tipo;
    }

    public int getTipo(){
        return tipo;
    }

    public void setContato(Contato contato){
        this.contato = contato;
    }
    public Contato getContato(){
        return contato;
    }

    abstract String mostraDados();




}
